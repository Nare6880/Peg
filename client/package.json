{
  "name": "caring-client",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "main": "build/electron/main.js",
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "^15.12.2",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.8",
    "electron-builder": "^22.14.5",
    "electron-rebuild": "^3.2.7",
    "postcss": "^8.4.5",
    "solid-js": "^1.3.4",
    "solid-scripts": "^0.0.61",
    "tailwindcss": "^3.0.18",
    "typescript": "4.3.2",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@types/electron-devtools-installer": "^2.2.1",
    "dlv": "^1.1.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "nanoid": "^3.2.0",
    "node-disk-info": "^1.3.0",
    "sass": "^1.49.0",
    "solid-app-router": "^0.2.1",
    "workbox-core": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5"
  },
  "scripts": {
    "start": "solid-scripts start",
    "build": "solid-scripts build",
    "test": "solid-scripts test",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm run build && tsc -p electron && electron-builder"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    "Chrome 74",
    "Firefox 63",
    "Safari 11",
    "Edge 17",
    "Node 10"
  ]
}
